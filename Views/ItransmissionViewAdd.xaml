<UserControl x:Class="gestion_concrets.Views.ItransmissionViewAdd"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:gestion_concrets.Views"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             mc:Ignorable="d"
             d:DesignHeight="1650" d:DesignWidth="800"
             >
    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
    <Grid Background="White">


        <!-- Itransmission Section -->
        <Label Content="I - Fampiatiana" HorizontalAlignment="Left" Margin="45,41,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
        <!-- I11 -->
        <Label Content="I11. Fanilikiliana: Efa niharan'ny fanilikilihana mifototra amin'ny fahasembanana ve ianao ?&#xD;&#xA;" HorizontalAlignment="Left" Margin="54,77,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ComboBox ItemsSource="{Binding Marginalization}" HorizontalAlignment="Left" Margin="54,111,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I11, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I12 -->
        <Label Content="I12. raha eny &quot;eny&quot; : hazavao" HorizontalAlignment="Left" Margin="54,157,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,191,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I12, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="671" Height="29" RenderTransformOrigin="0.5,0.5">
                <ui:TextBox.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleY="-0.205"/>
                        <RotateTransform/>
                        <TranslateTransform Y="-0.663"/>
                    </TransformGroup>
                </ui:TextBox.RenderTransform>
            </ui:TextBox>
            <!-- I2 -->
        <Label Content="I2. Inona no nataonao tamin'izany ?" HorizontalAlignment="Left" Margin="54,237,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,271,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I2, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="667" Height="29"/>
        <!-- I3 -->
        <Label Content="I3. Inona no sosokevitrao mba ho vahaolana amin'izany fanilikilihana izany ?" HorizontalAlignment="Left" Margin="54,317,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,351,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I3, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="667" Height="29"/>
        <!-- I4 -->
        <Label Content="I4. Inona no andraikitra ho raisinao hanafoanana izay fanilikilihana ny olona manana fahasembanana izay ? " HorizontalAlignment="Left" Margin="54,397,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,431,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I4, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="667" Height="29"/>
        <!-- I51 -->
        <Label Content="I51. Fandraisana anjara / Ianao ve efa nandray anjara tamin'ny : Fankanevitra ? " HorizontalAlignment="Left" Margin="54,477,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding  I51Options}" HorizontalAlignment="Left" Margin="54,511,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I51, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I52 -->
        <Label Content="I52. Fanadihadiana ?" HorizontalAlignment="Left" Margin="54,557,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I52Options}" HorizontalAlignment="Left" Margin="54,591,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I52, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I53 -->
        <Label Content="I53. Atrikasa ?" HorizontalAlignment="Left" Margin="54,637,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I53Options}" HorizontalAlignment="Left" Margin="54,671,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I53, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I54 -->
        <Label Content="I54. Fiofanana ?" HorizontalAlignment="Left" Margin="54,717,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I54Options}" HorizontalAlignment="Left" Margin="54,751,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I54, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I55 -->
        <Label Content="I55. Fivoriana samihafa ?" HorizontalAlignment="Left" Margin="54,797,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I55Options}" HorizontalAlignment="Left" Margin="54,831,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I55, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I56 -->
        <Label Content="I56. Hestika eny anivon'ny fokontany ?" HorizontalAlignment="Left" Margin="54,877,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I56Options}" HorizontalAlignment="Left" Margin="54,911,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I56, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I57 -->
        <Label Content="I57. Hetsika fanentanana ?" HorizontalAlignment="Left" Margin="54,957,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I57Options}" HorizontalAlignment="Left" Margin="54,991,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I57, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I58 -->
        <Label Content="I58. Hetsika eny anivon'ny fokontany ?" HorizontalAlignment="Left" Margin="54,1037,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I58Options}" HorizontalAlignment="Left" Margin="54,1073,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I58, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I59 -->
        <Label Content="I59. Rafitra samihafa ?" HorizontalAlignment="Left" Margin="54,1119,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I59Options}" HorizontalAlignment="Left" Margin="54,1153,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I59, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I510 -->
        <Label Content="I510. Fikambanana ?" HorizontalAlignment="Left" Margin="54,1199,0,0" VerticalAlignment="Top" FontSize="14"/>
            <ComboBox ItemsSource="{Binding I510Options}" HorizontalAlignment="Left" Margin="54,1233,0,0" VerticalAlignment="Top" Width="120" Height="32" SelectedItem="{Binding Itransmission.I510, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <!-- I6 -->
        <Label Content="I6. Raha &quot;tsia&quot;, inona no antony ?" HorizontalAlignment="Left" Margin="54,1277,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,1311,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I6, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="371" Height="29"/>
        <!-- I7 -->
        <Label Content="I7. Inona no heverinao ho sakana tsy ahafahanan'ny olona manana fahasembanana mandray anjara feno &#xD;&#xA;eny anivon'ny fiarahamonina ?" HorizontalAlignment="Left" Margin="54,1354,0,0" VerticalAlignment="Top" FontSize="14" Height="45"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,1399,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I7, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="682" Height="29"/>
        <!-- I8 -->
        <Label Content="I8. Inona no sosokevitra avy aminao mba hanamafisana fandraisan'anjaran'ny olona manana fahasembanana &#xD;&#xA;amin'ny sehatra samihafa ?" HorizontalAlignment="Left" Margin="54,1437,0,0" VerticalAlignment="Top" FontSize="14" Height="55"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,1482,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I8, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="682" Height="29"/>
        <!-- I9 -->
        <Label Content="I9. Inona no andraikitra ho raisinao mba ho fanamafisana ny fandraisanao anjara aminn'ny&#xD;&#xA; sehatra eny anivon'ny fiarahamonina ?" HorizontalAlignment="Left" Margin="54,1524,0,0" VerticalAlignment="Top" FontSize="14" Height="46"/>
        <ui:TextBox HorizontalAlignment="Left" Margin="54,1570,0,0" TextWrapping="Wrap" Text="{Binding Itransmission.I9, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="682" Height="29"/>

    </Grid>
    </ScrollViewer>
</UserControl>
